{
  "name": "navalon-client",
  "version": "1.0.0",
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "start": "expo start",
    "dev": "concurrently \"npx graphql-codegen -w\"  \"expo start --web\"",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "ab": "graphql-codegen --config codegen.ts"
  },
  "dependencies": {
    "@apollo/client": "^3.7.10",
    "@expo/webpack-config": "^18.0.1",
    "@graphql-codegen/client-preset": "^2.1.1",
    "@types/react": "~18.0.27",
    "concurrently": "^7.6.0",
    "expo": "~48.0.6",
    "expo-status-bar": "~1.4.4",
    "graphql": "^15.8.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-native": "0.71.3",
    "react-native-web": "~0.18.10"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@graphql-codegen/cli": "3.2.2",
    "typescript": "^5.0.2",
    "@graphql-codegen/introspection": "3.0.1",
    "@graphql-codegen/client-preset": "2.1.1"
  },
  "eslintConfig": {
    "extends": "universe/native",
    "rules": {
      "@typescript-eslint/no-unused-vars": "off",
      "indent": ["error", 2]
      }
  },
  "private": true
}